{

    "manifest_version": 2,
    "name": "ToDoList",
    "version": "0.1",
  
    "description": "A simple To Do List for quick lookup.",
  
    "icons": {
        "48": "Images/icon-48.png"
    },
  
    
    "browser_action":{
      
      "default_icon":"Images/icon-48.png",
      "default_title":"To Do List",
      "default_popup": "ToDoList.html"
    },
    "permissions": ["storage","notifications"]
  
}